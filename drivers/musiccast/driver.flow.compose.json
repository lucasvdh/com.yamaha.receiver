{
  "triggers": [
    {
      "id": "musiccast_input_changed",
      "title": {
        "en": "Input was changed",
        "nl": "Input is gewijzigd",
        "de": "Eingabe wurde geändert"
      },
      "tokens": [
        {
          "name": "input",
          "type": "string",
          "title": {
            "en": "Selected input",
            "nl": "Geselecteerde input",
            "de": "Ausgewählter Eingang"
          },
          "example": "HDMI1"
        }
      ],
      "args": []
    },
    {
      "id": "surround_program_changed",
      "title": {
        "en": "Surround program was changed",
        "nl": "Surround-programma is gewijzigd",
        "de": "Surround-Programm wurde geändert"
      },
      "tokens": [
        {
          "name": "surround_program",
          "type": "string",
          "title": {
            "en": "Selected surround program",
            "nl": "Geselecteerde surround-programma",
            "de": "Ausgewähltes Surround-Programm"
          },
          "example": "5ch Stereo"
        }
      ],
      "args": []
    },
    {
      "id": "muted",
      "title": {
        "en": "Device was muted",
        "nl": "Apparaat is gedempt",
        "de": "Gerät wurde stummgeschaltet"
      },
      "args": []
    },
    {
      "id": "unmuted",
      "title": {
        "en": "Device was unmuted",
        "nl": "Apparaat niet langer gedempt",
        "de": "Gerät war nicht stummgeschaltet"
      },
      "args": []
    }
  ],
  "actions": [
    {
      "id": "change_input",
      "title": {
        "en": "Change input",
        "nl": "Wijzig bron",
        "de": "Quelle ändern"
      },
      "args": [
        {
          "name": "input",
          "type": "autocomplete",
          "placeholder": {
            "en": "Input",
            "nl": "Input",
            "de": "Eingang"
          }
        }
      ]
    },
    {
      "id": "change_surround_program",
      "title": {
        "en": "Change surround program",
        "nl": "Wijzig surround-programma",
        "de": "Ändern Sie das Surround-Programm"
      },
      "args": [
        {
          "name": "surround_program",
          "type": "autocomplete",
          "placeholder": {
            "en": "Surround program",
            "nl": "Surround-programma",
            "de": "Surround-Programm"
          }
        }
      ]
    },
    {
      "id": "volume_up",
      "title": {
        "en": "Raise volume",
        "nl": "Zet geluid harder"
      },
      "args": [
        {
          "name": "volume",
          "type": "range",
          "min": 0,
          "max": 100,
          "step": 1,
          "label": "dB"
        }
      ]
    },
    {
      "id": "volume_down",
      "title": {
        "en": "Lower volume",
        "nl": "Zet geluid zachter"
      },
      "args": [
        {
          "name": "volume",
          "type": "range",
          "min": 0,
          "max": 100,
          "step": 1,
          "label": "dB"
        }
      ]
    }
  ]
}